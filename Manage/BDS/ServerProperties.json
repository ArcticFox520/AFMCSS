[
    {
        "ConfigurationName": "服务器名称",
        "ConfigurationItem": "server-name",
        "type": "String",
        "Description": "作为服务器在游戏内的服务器列表中显示的名称。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "游戏模式",
        "ConfigurationItem": "gamemode",
        "type": "Options",
        "OptionsName": [
            "生存",
            "创造",
            "冒险"
        ],
        "Options": [
            "survival",
            "creative",
            "adventure"
        ],
        "Description": "设定新玩家的游戏模式。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "游戏难度",
        "ConfigurationItem": "difficulty",
        "type": "Options",
        "Options": [
            "peaceful",
            "easy",
            "normal",
            "hard"
        ],
        "Description": "设定新玩家的游戏模式。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "地图类型",
        "ConfigurationItem": "level-type",
        "type": "Options",
        "Options": [
            "FLAT",
            "LEGACY",
            "DEFAULT"
        ],
        "Description": "决定生成的地图的类型。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "最大玩家人数",
        "ConfigurationItem": "max-players",
        "type": "Number",
        "Description": "设置服务器同时能容纳的最大玩家数量。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "服务器端口[IPv4]",
        "ConfigurationItem": "server-port",
        "type": "Number",
        "Description": "设置服务器监听的IPv4端口号。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "服务器端口[IPv6]",
        "ConfigurationItem": "server-portv6",
        "type": "Number",
        "Description": "设置服务器监听的IPv6端口号。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "服务器存档",
        "ConfigurationItem": "level-name",
        "type": "String",
        "Description": "该值用于存档的名称以及文件夹名。\n你可以复制你的存档到这里，并把该值修改成文件夹的名称，以此替换服务器加载的存档。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "世界种子",
        "ConfigurationItem": "level-seed",
        "type": "String",
        "Description": "用于将世界随机化。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "正版验证",
        "ConfigurationItem": "online-mode",
        "type": "Boolean",
        "Description": "是否开启正版验证。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "白名单",
        "ConfigurationItem": "allow-list",
        "type": "Boolean",
        "Description": "是否开启白名单",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "最大可视距离",
        "ConfigurationItem": "view-distance",
        "type": "Number",
        "Description": "设置玩家可视距离最大值\n单位为区块",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "空闲踢出时间",
        "ConfigurationItem": "player-idle-timeout",
        "type": "Boolean",
        "Description": "玩家空闲时间到达设定值后将被踢出，如果设置为0或被移除\n单位为分钟。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "最大线程数",
        "ConfigurationItem": "max-threads",
        "type": "Number",
        "Description": "设置服务器尝试使用的最大线程数。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "停载区块大小",
        "ConfigurationItem": "tick-distance",
        "type": "Number",
        "Description": "当玩家远离某个区块的距离超过设定值后，该区块会停止加载。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "新玩家权限",
        "ConfigurationItem": "default-player-permission-level",
        "type": "Options",
        "Options": [
            "visitor",
            "member",
            "operator"
        ],
        "Description": "新玩家加入时的权限。",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "强载资源包",
        "ConfigurationItem": "texturepack-required",
        "type": "Boolean",
        "Description": "是否强制加载资源包",
        "Source": "Minecraft Wiki"
    },
    {
        "ConfigurationName": "错误日志",
        "ConfigurationItem": "content-log-file-enabled",
        "type": "Boolean",
        "Description": "是否启用错误日志",
        "Source": "Minecraft Wiki"
    }
]